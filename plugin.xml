<?xml version="1.0" encoding="UTF-8"?>headers

<plugin xmlns="http://www.w3.org/ns/widgets"
    id="nat-network-stream"
    version="0.1.0">
    <name>nat-network-stream</name>
    <description></description>
    <license>MIT</license>
    <keywords>weex,plugin,nat,natjs,network,fetch</keywords>
    <repo>https://github.com/Natjs/weex-nat-network-stream.git</repo>
    <issue>https://github.com/Natjs/weex-nat-network-stream/issues</issue>
    
     <!-- ios -->
     <platform name="ios">
         <framework src="NatStream" type="podspec" spec="" />
         <config-file target="WeexpluginConfig.xml" parent="/*">
             <feature name="WXTestModule">
                 <param name="category" value="module" />
                 <param name="ios-package" value="WeexNatStream" />
                 <param name="api" value="nat/network/stream" />
             </feature>
         </config-file>
         <header-file src="ios/WeexNatStream.h"/>
         <source-file src="ios/WeexNatStream.m"/>
     </platform>

     <!-- android -->
     <platform name="android">
     </platform>

</plugin>
